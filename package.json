{
  "name": "e-tools",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.1",
  "description": "A modern TypeScript library",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "converage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "lint-staged": "lint-staged",
    "test:staged": "vitest related --run",
    "coverage:staged": "vitest related --coverage --run",
    "precommit": "run-p lint-staged typecheck"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.7.5",
    "@vitest/coverage-v8": "2.1.2",
    "bumpp": "^9.7.1",
    "eslint": "^9.12.0",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "unbuild": "^3.0.0-rc.11",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm precommit"
  },
  "lint-staged": {
    "*": "eslint --fix",
    "test/**/*.ts": [
      "pnpm test:staged",
      "pnpm coverage:staged"
    ]
  }
}
